all:: xgvis xgobi

foo:
	echo $(USRLIBDIR)
	echo "$(LDPRELIBS) $(origin LDPRELIBS)"

xgobi:  $(XGOBIOBJ)
	  $(RM) $@
	 $(CCLINK) -o $@ $(LDOPTIONS)  $(XGOBIOBJ)   $(LDLIBS)  $(CORBA_LIBRARIES) $(LOCAL_LIBRARIES) $(EXTRA_LOAD_FLAGS)

clean::
	 $(RM) xgobi

xgvis:  $(XGVISOBJ)
	  $(RM) $@
	 $(CCLINK) -o $@ $(LDOPTIONS)  $(XGVISOBJ)   $(LDLIBS)  $(LOCAL_LIBRARIES) $(EXTRA_LOAD_FLAGS)

clean::
	 $(RM) xgvis

purexgobi: xgobi.o $(JOINTOBJ)
	purify -cache-dir=/tmp  -always-use-cache-dir=yes \
	${CC} ${CFLAGS} ${LD} ${LDOPTIONS} -o xgobi ${XGOBIOBJ} ${LOCAL_LIBRARIES} \
	${SYS_LIBRARIES}

purexgvis: xgvis.o $(XGVISOBJ)
	purify -cache-dir=/tmp  -always-use-cache-dir=yes \
	${CC} ${CFLAGS} ${LD} ${LDOPTIONS} -o xgvis ${XGVISOBJ} ${LOCAL_LIBRARIES} \
	${SYS_LIBRARIES}

